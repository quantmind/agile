{
    "python": {
        "agile_version": "agile.__version__"
    },
    "github": {
        "pythontest": {
            "version": "{{ agile_version }}",
            "release_notes": "tests/history",
            "release_notes_file": "test_release_note.md"
        },
        "nodetest": {
            "version": "{{ tests.configs.release.version }}"
        }
    },
    "s3": {
        "upload": {
            "files": {}
        }
    },
    "tasks": {
        "badtask": ["pythontest"],
        "releasepy": {
            "command": [
                "python",
                "github:pythontest"
            ]
        },
        "upload": {
            "command": [
                {
                    "command": "s3:upload",
                    "when": "cfg.push"
                }
            ]
        }
    }
}
